unbak() {
  local FILE

  for FILE in "$@"
  do
    if [[ -e "$FILE" ]]; then
      case "$FILE" in
        *.BAK)
          mv $FILE `echo $FILE | sed -e 's/.BAK//'`
          ;;
        *.bak)
          mv $FILE `echo $FILE | sed -e 's/.bak//'`
          ;;
        *)
          return 1
          ;;
      esac
    else
      echo "$FILE not found";
    fi
  done
}
