unbak() {
  local FILE

  for FILE in "$@"; do
    if [[ -e "$FILE" ]]; then
      case "$FILE" in
        *.BAK)
          mv "$FILE" "${FILE/.BAK/}"
          ;;
        *.bak)
          mv "$FILE" "${FILE/.bak/}"
          ;;
        *)
          return 1
          ;;
      esac
    else
      echo "$FILE not found"
    fi
  done
}
